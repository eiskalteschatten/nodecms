{
  "name": "NodeCms",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "start:dev": "NODE_ENV=development ./node_modules/.bin/nodemon -e njk,js,json --ignore 'public/**/*.js' --inspect",
    "start:staging": "NODE_ENV=staging npm start",
    "start:prod": "NODE_ENV=production npm start",
    "docker": "bash -c \"trap 'docker-compose down' EXIT; docker-compose up\"",
    "docker:rebuild": "bash -c \"trap 'docker-compose down' EXIT; docker-compose up --build\"",
    "docker:start": "bash -c \"trap 'docker-compose down' EXIT; docker-compose up\"",
    "cleanup": "node -e 'require(\"./src/lib/exitCleanup\")()'",
    "test": "npm run lint",
    "lint": "./node_modules/.bin/eslint .",
    "precommit": "npm test"
  },
  "dependencies": {
    "apicache": "^0.8.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "compile-sass": "^0.1.1",
    "compression": "^1.7.4",
    "concatenate-js-middleware": "^0.0.4",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.4",
    "express-session": "^1.16.1",
    "jquery": "^3.4.0",
    "marked": "^0.7.0",
    "moment": "^2.24.0",
    "mongoose": "^5.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "nunjucks": "^3.2.0",
    "nunjucks-markdown": "^2.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "rss": "^1.2.2",
    "serve-favicon": "^2.5.0",
    "simplemde": "^1.11.2",
    "slug": "^0.9.4",
    "uikit": "^3.0.3",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "debug": "^4.1.1",
    "eslint": "^4.16.0",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "nodemon": "^1.18.11"
  }
}
