{
  "name": "HistoryMuseum",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node ./src/bin/www",
    "start:dev": "NODE_ENV=development ./node_modules/.bin/nodemon -e njk,js,json --ignore 'public/**/*.js' --inspect",
    "start:staging": "NODE_ENV=staging npm start",
    "start:prod": "NODE_ENV=production npm start",
    "docker": "bash -c \"trap 'docker-compose down' EXIT; docker-compose up\"",
    "docker:rebuild": "bash -c \"trap 'docker-compose down' EXIT; docker-compose up --build\"",
    "docker:start": "bash -c \"trap 'docker-compose down' EXIT; docker-compose up\"",
    "cleanup": "node -e 'require(\"./src/lib/exitCleanup\")()'",
    "test": "npm run lint",
    "lint": "./node_modules/.bin/eslint .",
    "precommit": "npm test"
  },
  "dependencies": {
    "apicache": "^0.8.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.15.1",
    "compile-sass": "^0.0.3",
    "compression": "^1.7.1",
    "concatenate-js-middleware": "^0.0.2",
    "connect-flash": "^0.1.1",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "~1.4.3",
    "express": "~4.13.4",
    "express-session": "^1.15.6",
    "jquery": "^3.3.1",
    "marked": "^0.3.12",
    "mongoose": "^5.0.4",
    "morgan": "~1.7.0",
    "node-sass": "^4.7.2",
    "nunjucks": "^2.5.2",
    "nunjucks-markdown": "^2.0.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "slug": "^0.9.1",
    "uikit": "^3.0.0-beta.39",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "debug": "~2.2.0",
    "eslint": "^4.16.0",
    "eslint-plugin-promise": "^3.6.0",
    "husky": "^0.14.3",
    "nodemon": "^1.14.12"
  }
}
